@using LakewoodscoopScraper.data
@model IEnumerable<Lakewoodscoop>

<div class="container">
    <img src="http://www.thelakewoodscoop.com/news/wp-includes/images/thelakewoodscoop_logo.png" class="img-responsive" style="margin: 0 auto; width:20%" />
    <br />
    <br />
    @foreach (var l in Model)
    {
        <div class="col-md-8 col-md-offset-2 well">
            <h3 style="text-align:center"><a href="@l.TitleURL">@l.Title</a></h3>
            <br />
            <img src="@l.ImageURL" class="img-responsive" style="margin: 0 auto;" />
            <br />
            <h4 style="text-align:center">@l.Blog</h4>
        </div>
    }
</div>







